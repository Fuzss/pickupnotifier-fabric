{
  "schemaVersion": 1,
  "id": "${modId}",
  "version": "${modVersion}",

  "name": "${modName}",
  "description": "${modDescription}",
  "authors": [
    "${modAuthor}"
  ],
  "contact": {
    "homepage": "${modPageUrl}",
    "issues": "${modIssueUrl}"
  },

  "license": "Mozilla Public License 2.0",
  "icon": "avatar.png",

  "environment": "*",
  "entrypoints": {
    "main": [
      "${mainEntryPoint}"
    ],
    "client": [
      "${clientEntryPoint}"
    ],
    "modmenu": [
      "fuzs.pickupnotifier.compat.ModMenuMenu"
    ]
  },
  "mixins": [
    "${modId}.mixins.json"
  ],

  "depends": {
    "fabricloader": ">=${minFabricVersion}",
    "fabric": "*",
    "minecraft": ">=${minMinecraftVersion} <${nextMinecraftVersion}",
    "java": ">=16"
  }
}
